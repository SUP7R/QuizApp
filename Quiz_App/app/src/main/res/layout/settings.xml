<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<ScrollView
		android:id="@+id/vscroll1"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:background="#FFFFFF">
		<LinearLayout
			android:id="@+id/linear1"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:paddingLeft="8dp"
			android:paddingTop="25dp"
			android:paddingRight="8dp"
			android:paddingBottom="25dp"
			android:gravity="center_horizontal"
			android:orientation="vertical">
			<ImageView
				android:id="@+id/imageview1"
				android:layout_width="170dp"
				android:layout_height="170dp"
				android:background="#FFFFFF"
				android:src="@drawable/icon"
				android:scaleType="centerCrop"/>
			<TextView
				android:id="@+id/textview1"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginTop="8dp"
				android:padding="4dp"
				android:text="اختبر نفسك"
				android:textSize="30sp"
				android:textStyle="bold"
				android:textColor="#282250"/>
			<LinearLayout
				android:id="@+id/linear3"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginLeft="8dp"
				android:layout_marginRight="8dp"
				android:padding="4dp"
				android:gravity="right|center_vertical"
				android:orientation="horizontal">
				<TextView
					android:id="@+id/textview3"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginRight="5dp"
					android:paddingLeft="8dp"
					android:paddingTop="4dp"
					android:paddingRight="4dp"
					android:paddingBottom="4dp"
					android:text="مراسلة "
					android:textSize="16sp"
					android:textStyle="bold"
					android:textColor="#9E9E9E"/>
				<ImageView
					android:id="@+id/imageview2"
					android:layout_width="28dp"
					android:layout_height="28dp"
					android:src="@drawable/comment"
					android:scaleType="fitCenter"/>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/linear_a1"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginLeft="16dp"
				android:layout_marginRight="16dp"
				android:layout_marginBottom="16dp"
				android:padding="8dp"
				android:background="#F5F5F5"
				android:gravity="center_vertical"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/imageview19"
					android:layout_width="25dp"
					android:layout_height="25dp"
					android:src="@drawable/icon_15"
					android:scaleType="fitCenter"
					android:rotation="180"/>
				<TextView
					android:id="@+id/textview4"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:text="جروب واتساب"
					android:textSize="16sp"
					android:textColor="#282250"
					android:layout_weight="1"/>
				<ImageView
					android:id="@+id/imageview3"
					android:layout_width="30dp"
					android:layout_height="30dp"
					android:padding="2dp"
					android:src="@drawable/whatsapp"
					android:scaleType="fitCenter"/>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/linear11"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginLeft="8dp"
				android:layout_marginRight="8dp"
				android:padding="4dp"
				android:gravity="right|center_vertical"
				android:orientation="horizontal">
				<TextView
					android:id="@+id/textview9"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginRight="5dp"
					android:paddingLeft="8dp"
					android:paddingTop="4dp"
					android:paddingRight="4dp"
					android:paddingBottom="4dp"
					android:text="التطبيق"
					android:textSize="16sp"
					android:textStyle="bold"
					android:textColor="#9E9E9E"/>
				<ImageView
					android:id="@+id/imageview8"
					android:layout_width="28dp"
					android:layout_height="28dp"
					android:src="@drawable/icon_6"
					android:scaleType="fitCenter"/>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/linear_a3"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginLeft="16dp"
				android:layout_marginRight="16dp"
				android:paddingTop="8dp"
				android:paddingBottom="8dp"
				android:background="#F5F5F5"
				android:orientation="vertical">
				<LinearLayout
					android:id="@+id/linear14"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:paddingLeft="8dp"
					android:paddingRight="8dp"
					android:gravity="center_vertical"
					android:orientation="horizontal">
					<ImageView
						android:id="@+id/imageview16"
						android:layout_width="25dp"
						android:layout_height="25dp"
						android:src="@drawable/icon_15"
						android:scaleType="fitCenter"
						android:rotation="180"/>
					<TextView
						android:id="@+id/textview11"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:text="معلومات حسابك"
						android:textSize="16sp"
						android:textColor="#282250"
						android:layout_weight="1"/>
					<ImageView
						android:id="@+id/imageview14"
						android:layout_width="30dp"
						android:layout_height="30dp"
						android:padding="2dp"
						android:src="@drawable/icon_7"
						android:scaleType="fitCenter"/>
				</LinearLayout>
				<LinearLayout
					android:id="@+id/linear10"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:paddingLeft="8dp"
					android:paddingRight="8dp"
					android:gravity="center_vertical"
					android:orientation="horizontal">
					<ImageView
						android:id="@+id/imageview11"
						android:layout_width="25dp"
						android:layout_height="25dp"
						android:src="@drawable/icon_15"
						android:scaleType="fitCenter"
						android:rotation="180"/>
					<TextView
						android:id="@+id/textview12"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:text="معلومات عن التطبيق "
						android:textSize="16sp"
						android:textColor="#282250"
						android:layout_weight="1"/>
					<ImageView
						android:id="@+id/imageview7"
						android:layout_width="30dp"
						android:layout_height="30dp"
						android:padding="2dp"
						android:src="@drawable/icon_6_2"
						android:scaleType="fitCenter"/>
				</LinearLayout>
				<LinearLayout
					android:id="@+id/linear21"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:paddingLeft="8dp"
					android:paddingRight="8dp"
					android:gravity="center_vertical"
					android:orientation="horizontal">
					<ImageView
						android:id="@+id/imageview22"
						android:layout_width="25dp"
						android:layout_height="25dp"
						android:src="@drawable/icon_15"
						android:scaleType="fitCenter"
						android:rotation="180"/>
					<TextView
						android:id="@+id/textview14"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:text="تسجيل الخروج"
						android:textSize="16sp"
						android:textColor="#282250"
						android:layout_weight="1"/>
					<ImageView
						android:id="@+id/imageview23"
						android:layout_width="30dp"
						android:layout_height="30dp"
						android:padding="2dp"
						android:src="@drawable/icon_6_2"
						android:scaleType="fitCenter"/>
				</LinearLayout>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/linear_logo"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="8dp"
				android:gravity="center_horizontal|center_vertical"
				android:orientation="vertical">
				<ImageView
					android:id="@+id/imageview_arpo"
					android:layout_width="35dp"
					android:layout_height="35dp"
					android:src="@drawable/crown"
					android:scaleType="fitCenter"
					android:translationY="7.0dp"/>
				<TextView
					android:id="@+id/textview_arpo"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="Super"
					android:textSize="12sp"
					android:textColor="#424242"/>
			</LinearLayout>
		</LinearLayout>
	</ScrollView>
</LinearLayout>
